import { environment } from './environments/environment';
import './popup.scss';

const el = document.querySelector('#popup-vp');

async function getNotice(): Promise<string> {
  return Promise.resolve('<%= className %> works');
}

async function main() {
  if (el) {
    el.textContent = environment.production
      ? 'Production Configuration'
      : await getNotice();
  }
}

void main();
