{
  "$schema": "http://json-schema.org/schema",
  "name": "cli",
  "version": "0.0.1",
  "generators": {
    "init": {
      "factory": "./src/generators/init/init",
      "schema": "./src/generators/init/schema.json",
      "description": "init generator",
      "hidden": true
    },
    "application": {
      "factory": "./src/generators/application/application",
      "schema": "./src/generators/application/schema.json",
      "aliases": ["app"],
      "x-type": "application",
      "description": "Create a cli application"
    },
    "command": {
      "factory": "./src/generators/command/command",
      "schema": "./src/generators/command/schema.json",
      "description": "command generator"
    },
    "refresh": {
      "factory": "./src/generators/refresh/refresh",
      "schema": "./src/generators/refresh/schema.json",
      "description": "refresh generator"
    }
  }
}
